package DIC_28;

class Employee1{
	private String employeename;
	private int employeeid;
	private double salry;
	
	
	public Employee1(int employeeid, String employeename) {
		this.employeename = employeename;
		this.employeeid = employeeid;
	}
	

public double  calculateSalary() {
	return salry;
	
	}

}
class generatePayStub extends Employee{

	public generatePayStub(int employeeid, String employeename) {
		super(employeeid, employeename);
		
	}
	
}
public class Manager1 {

	public static void main(String[] args) 
	
	{
		Employee e = new  Employee(1200, "Abhishek");
		e.calculateSalary();
		e.equals(e);
	 

	}

}
