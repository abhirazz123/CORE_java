package OCt_29;

class calculate
{
	public int calculateArea () {
		int sideLength = 0;
		if(sideLength<0) {
		
		}else {
			System.out.println("error");
		}
		return sideLength*sideLength;
	}
		
	
}

public class ShapeCalculator {
